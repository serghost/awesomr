<header>
  <%= link_to logo, root_path %>
  <nav class="round">
    <ul>
      <li><%= link_to t("pages.title.home"), root_path %></li>
      <% if signed_in? %>
        <li><%= link_to t("users.title.profile"), current_user %></li>
        <li><%= link_to t("users.title.all"), users_path %></li>
        <li><%= link_to t("users.title.tweaks"), edit_user_path(current_user) %></li>
      <% end %>
      <li><%= link_to t("pages.title.help"), help_path %></li>
      <% if signed_in? %>
        <li><%= link_to t("users.title.sign_out"), signout_path %></li>
      <% else %>
        <li><%= link_to t("users.title.sign_in"), signin_path %></li>
      <% end %>
    </ul>
  </nav>
</header>
